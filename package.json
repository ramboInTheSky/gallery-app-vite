{
  "name": "gallery-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky && husky install",
    "postinstall": "npx playwright install",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "start-server-and-test 'npm start' http://localhost:4173 'playwright test'",
    "test:e2e:ui": "start-server-and-test 'npm start' http://localhost:4173 'playwright test --ui'",
    "start": "npm run build && npm run preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@tanstack/query-sync-storage-persister": "^5.64.2",
    "@tanstack/react-query": "^5.64.2",
    "@tanstack/react-query-devtools": "^5.64.2",
    "@tanstack/react-query-persist-client": "^5.64.2",
    "dotenv": "^16.4.7",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.3",
    "vitest": "^3.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@emotion/jest": "^11.13.0",
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.50.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "jsdom": "^26.0.0",
    "start-server-and-test": "^2.0.10",
    "ts-jest": "^29.2.5",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-environment": "^1.1.3"
  },
  "engines": {
    "node": "22.11.0",
    "npm": "10.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
